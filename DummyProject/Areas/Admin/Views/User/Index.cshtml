<div class="row">
    <div class="col-8">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-4">
        <a asp-page="/Account/Register" asp-area="identity" class="btn btn-info form-control">
        <i class="fas fa-plus"></i>&nbsp;Add New User    
        </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tblData" class="table table-bordered table-hover">
        <thead>
            <<tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/User.js"></script>
}